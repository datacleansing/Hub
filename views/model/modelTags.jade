extends modelDetailBase

block model_detail
  if items.length == 0
    h5.text-center No data yet
  else
    table(
      class="table table-hover"
      id='modelTagsTable'
    )
      each val in items
        tr
          td
            a(href= URI_REPO + repoId + URI_MODELS + "/" + model.id + URI_TAGS + "/" + val.id)
              text=  val.id

block actionContent
  div(class="panel panel-default")
    .panel-body
      ul(class='list-group')
        a(
          class='list-group-item'
          href= URI_REPO + repoId + URI_MODELS + "/" + model.id + URL_TAG_LATEST)
            = items.length == 0 ? "Add Data" : "Update Data"

extends modelDetailBase

block model_detail
  table(
    class="table table-hover"
    id='modelTagsTable'
  )
    each val in items
      tr
        td
          a(href= "/ui/archives/model/" + val.key)
            text=  val.id

block actionContent
  div(class="panel panel-default")
    .panel-body
      ul(class='list-group')
        a(
          class='list-group-item'
          href= URI_REPO + repoId + URI_MODELS + "/" + model.id + "/newTag")  Update Data

extends dmcloud_base

mixin dashboard(id, des, url)
  div(class="col-md-6 total")
    div.count
      h5= des
      a(
        id=id
        href=url
        ) 0

block body
  .container
    section(class="number text-center")
      .row
        +dashboard('model', 'Your Customized Models :', '/ui/models')
        +dashboard('service', 'Your Published Services:', '/ui/services')
